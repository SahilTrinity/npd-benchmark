
== create NPD

1) CREATE DATABASE npd_vig1uri_obda_scale1 WITH TEMPLATE = template0 ENCODING = 'UTF8' LC_COLLATE = 'en_US.UTF-8' LC_CTYPE = 'en_US.UTF-8';

2) ./executeSql.sh /data/davide/Scripts/postgres/EDBT/npd_no_spatial.sql npd_vig1uri_obda_scale50 postgres postgres

3) dlanti@obdalin3:/data/davide/Scripts/postgres/EDBT$ cd EDBTDumps/
                                ./pumpDataToNpd.sh npd_vig1uri_obda_scale5 postgres /data/davide/Scripts/postgres/EDBT/EDBTDumps/obda/npd5uri
   
4) ./executeSql.sh /data/davide/Scripts/postgres/EDBT/postgresindexes.sql npd_vig1uri_obda_scale5 postgres postgres

5) ./executeSql.sh /data/davide/Scripts/postgres/EDBT/postgresfkeys.sql npd_vig1uri_obda_scale5 postgres postgres

6) ./executeSql.sh /data/davide/Scripts/postgres/EDBT/vacuum.sql npd_vig1uri_obda_scale5 postgres postgres
